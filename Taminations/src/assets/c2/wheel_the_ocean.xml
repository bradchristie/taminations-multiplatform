<tamination title="Wheel the Ocean">

  <tam title="Wheel the Ocean" from="Couples Facing Out"
       formation="Couples Facing Out" parts="3">
    <path couples="3 1">
      <move select="Belle Wheel" />
      <move select="Extend Left" hands="right" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path couples="3 1">
      <move select="Beau Wheel" />
      <move select="Stand" beats="4" hands="nohands" />
    </path>
  </tam>

  <tam title="Wheel the Ocean"
       from="Lines Facing Out"
       formation="Lines Facing Out"
       fractions="3" >
    <path>
      <move select="Belle Wheel" />
      <move select="Extend Left" hands="right" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Beau Wheel" />
      <move select="Stand" beats="4" hands="nohands" />
    </path>
    <path>
      <move select="Belle Wheel" />
      <move select="Extend Left" hands="right" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Beau Wheel" />
      <move select="Stand" beats="4" hands="nohands" />
    </path>
  </tam>

  <tam title="Wheel the Ocean"
       from="Right-Hand Two-Faced Lines"
       formation="Two-Faced Lines RH"
       parts="3" >
    <path>
      <move select="Stand" beats="7" />
    </path>
    <path>
      <move select="Stand" beats="3" />
      <move select="Extend Left" hands="right" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Belle Wheel" />
      <move select="Extend Left" hands="right" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Beau Wheel" />
    </path>
  </tam>

  <tam title="Wheel the Ocean"
       from="Left-Hand Two-Faced Lines"
       formation="Two-Faced Lines LH"
       parts="3" >
    <path>
      <move select="Belle Wheel" />
      <move select="Extend Left" hands="right" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Beau Wheel" />
    </path>
    <path>
      <move select="Stand" beats="7" />
    </path>
    <path>
      <move select="Stand" beats="3" />
      <move select="Extend Left" hands="right" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
  </tam>

  <tam title="Wheel the Ocean"
       from="Trade By"
       formation="Trade By"
       parts="3" >
    <path>
      <move select="Belle Wheel" scaleX=".5" />
      <move select="Extend Left" hands="right" beats="2" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Beau Wheel" scaleX=".5" />
    </path>
    <path>
      <move select="Belle Wheel" scaleX=".5" />
      <move select="Extend Left" hands="right" beats="2" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Beau Wheel" scaleX=".5" />
    </path>
  </tam>

  <tam title="Wheel the Ocean"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru"
       parts="3" >
    <path>
      <move select="Belle Wheel" scaleX=".5" />
      <move select="Extend Left" hands="right" beats="2" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Beau Wheel" scaleX=".5" />
    </path>
    <path>
      <move select="Stand" beats="3" />
      <move select="Extend Left" hands="right" beats="2" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Stand" beats="3" />
      <move select="Stand" hands="nohands" beats="3" />
    </path>
  </tam>

  <tam title="Reverse Wheel the Ocean" from="Couples Facing Out"
       formation="Couples Facing Out" parts="3">
    <path>
      <move select="Belle Reverse Wheel" />
      <move select="Stand" beats="4" hands="nohands" />
    </path>
    <path>
      <move select="Beau Reverse Wheel" />
      <move select="Extend Right" hands="left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
  </tam>

  <tam title="Reverse Wheel the Ocean" from="Lines Facing Out"
       formation="Lines Facing Out" parts="3">
    <path>
      <move select="Belle Reverse Wheel" />
      <move select="Stand" beats="4" hands="nohands" />
    </path>
    <path>
      <move select="Beau Reverse Wheel" />
      <move select="Extend Right" hands="left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Belle Reverse Wheel" />
      <move select="Stand" beats="4" hands="nohands" />
    </path>
    <path>
      <move select="Beau Reverse Wheel" />
      <move select="Extend Right" hands="left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
  </tam>

  <tam title="Reverse Wheel the Ocean" from="Right-Hand Two-Faced Lines"
       formation="Two-Faced Lines RH" parts="3">
    <path>
      <move select="Stand" beats="3" hands="nohands" />
      <move select="Extend Right" hands="left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
    </path>
    <path>
      <move select="Belle Reverse Wheel" />
      <move select="Stand" beats="4" hands="nohands" />
    </path>
    <path>
      <move select="Beau Reverse Wheel" />
      <move select="Extend Right" hands="left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
  </tam>

  <tam title="Reverse Wheel the Ocean" from="Left-Hand Two-Faced Lines"
       formation="Two-Faced Lines LH" parts="3">
    <path>
      <move select="Belle Reverse Wheel" />
      <move select="Stand" beats="4" hands="nohands" />
    </path>
    <path>
      <move select="Beau Reverse Wheel" />
      <move select="Extend Right" hands="left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Stand" beats="3" hands="nohands" />
      <move select="Extend Right" hands="left" beats="3" scaleX="3" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
    </path>
  </tam>

  <tam title="Reverse Wheel the Ocean"
       from="Trade By"
       formation="Trade By"
       parts="3" >
    <path>
      <move select="Belle Reverse Wheel" scaleX=".5" />
    </path>
    <path>
      <move select="Beau Reverse Wheel" scaleX=".5" />
      <move select="Extend Right" hands="left" beats="2" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
      <move select="Belle Reverse Wheel" scaleX=".5" />
    </path>
    <path>
      <move select="Beau Reverse Wheel" scaleX=".5" />
      <move select="Extend Right" hands="left" beats="2" scaleY="2" />
      <move select="Forward" />
    </path>
  </tam>

  <tam title="Reverse Wheel the Ocean"
       from="Completed Double Pass Thru"
       formation="Completed Double Pass Thru"
       parts="3" >
    <path>
      <move select="Belle Reverse Wheel" scaleX=".5" />
    </path>
    <path>
      <move select="Beau Reverse Wheel" scaleX=".5" />
      <move select="Extend Right" hands="left" beats="2" scaleY="2" />
      <move select="Forward" />
    </path>
    <path>
    </path>
    <path>
      <move select="Stand" beats="3" />
      <move select="Extend Right" hands="left" beats="2" scaleY="2" />
      <move select="Forward" />
    </path>
  </tam>

  <tam title="Head Couples Wheel the Ocean"
       group=" "
       formation="Promenade"
       parts="2.25" >
    <path numbers="H H">
      <move select="BackHinge Right" scaleY=".5" />
      <move select="HalfBackHinge Right" />
      <move select="Stand" beats="3" offsetX="-.25" offsetY=".25" />
    </path>
    <path numbers="H H">
      <move select="Hinge Left" scaleY=".5" />
      <move select="HalfHinge Left" />
      <move select="Extend Left" hands="right" beats="4" scaleX="3" scaleY="2" offsetX="-.25" offsetY=".25" />
      <move select="Forward"/>
    </path>
    <path numbers="S S">
      <move select="HalfBackHinge Right" beats="2.25" scaleX=".5" scaleY=".5" />
      <move select="Stand" beats="3" offsetX="-.25" offsetY=".25" />
    </path>
    <path numbers="S S">
      <move select="HalfHinge Left" beats="2.25" scaleX=".5" scaleY=".5" />
      <move select="Extend Left" hands="right" beats="4" scaleX="3" scaleY="2" offsetX="-.25" offsetY="-.75" />
      <move select="Forward"/>
    </path>
  </tam>


  <tam title="As Couples Wheel the Ocean" group=" "
       formation="Lines Facing Out" fractions="5">
    <path>
      <move select="Belle Wheel" beats="5" scaleX="3" scaleY="3"/>
      <move select="Extend Left" beats="4" hands="gripleft" scaleX="2" scaleY="4"/>
      <move select="Forward 2" hands="both"/>
    </path>
    <path>
      <move select="Belle Wheel" beats="5" hands="both"/>
      <move select="Extend Left" beats="4" hands="gripright" scaleX="2" scaleY="4"/>
      <move select="Forward 2" hands="right"/>
    </path>
    <path>
      <move select="Beau Wheel" beats="5" scaleX=".5" hands="both"/>
      <move select="Stand" beats="6" hands="left"/>
    </path>
    <path>
      <move select="Beau Wheel" beats="5" scaleX="1.5" scaleY="3"/>
      <move select="Stand" beats="6" hands="right"/>
    </path>
  </tam>

  <tam title="All 8 Wheel the Ocean" group=" "
       formation="Static Facing Out" fractions="3">
    <path>
      <move select="Beau Wheel"/>
    </path>
    <path>
      <move select="Belle Wheel"/>
      <move select="Lead Left" beats="2" scaleX="2"/>
      <move select="Hinge Right"/>
      <move select="Forward 3"/>
    </path>
    <path>
      <move select="Beau Wheel"/>
    </path>
    <path>
      <move select="Belle Wheel"/>
      <move select="Lead Left" beats="2" scaleX="2"/>
      <move select="Hinge Right"/>
      <move select="Forward 3"/>
    </path>
  </tam>

  <tam title="All 8 Reverse Wheel the Ocean" from="Squared Couples Facing Out"
       formation="Static Facing Out" fractions="3" group=" " display="none">
    <path>
      <move select="Beau Reverse Wheel"/>
      <move select="Lead Right" beats="2" scaleX="2"/>
      <move select="Hinge Left"/>
      <move select="Forward 3"/>
    </path>
    <path>
      <move select="Belle Reverse Wheel"/>
    </path>
    <path>
      <move select="Beau Reverse Wheel"/>
      <move select="Lead Right" beats="2" scaleX="2"/>
      <move select="Hinge Left"/>
      <move select="Forward 3"/>
    </path>
    <path>
      <move select="Belle Reverse Wheel"/>
    </path>
  </tam>

</tamination>
