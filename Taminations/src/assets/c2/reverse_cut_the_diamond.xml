<tamination title="Reverse Cut the Diamond">

  <tam title="Reverse Cut the Diamond" from="Right-Hand Diamond">
    <formation>
      <dancer gender="boy" y="0" x="3" angle="270" />
      <dancer gender="girl" y="-1" x="0" angle="180" />
    </formation>
    <path>
      <move select="Lead Right" beats="4" scaleY="3" />
    </path>
    <path couples="3 1">
      <move select="Hinge Right" beats="1" />
      <move select="Lead Right" beats="3" scaleX="3" />
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond" from="Left-Hand Diamond"
    formation="Diamond LH">
    <path>
      <move select="Lead Left" beats="4" scaleY="3" />
    </path>
    <path>
      <move select="Hinge Left" beats="1" />
      <move select="Lead Left" beats="3" scaleX="3" />
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond" from="Facing Diamond, Left-Hand Centers"
       formation="Diamond Facing">
    <path>
      <move select="Lead Right" beats="4" scaleY="3"/>
    </path>
    <path>
      <move select="Hinge Left" beats="1"/>
      <move select="Lead Left" beats="3" scaleX="3"/>
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond" from="Facing Diamond, Right-Hand Centers"
        formation="Diamond Facing RH">
    <path>
      <move select="Lead Left" beats="4" scaleY="3" />
    </path>
    <path>
      <move select="Hinge Right" beats="1" />
      <move select="Lead Right" beats="3" scaleX="3" />
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond" from="Right-Hand Diamonds" formation="Diamonds RH Girl Points">
  <path>
    <move select="Hinge Right" beats="2" />
    <move select="Lead Right" beats="2" scaleX="1.5" />
  </path>
  <path>
    <move select="Lead Right" beats="4" scaleX=".5" scaleY="3" />
  </path>
  <path>
    <move select="Hinge Right" beats="2" />
    <move select="Lead Right" beats="2" scaleX="1.5" />
  </path>
  <path>
    <move select="Lead Right" beats="4" scaleX=".5" scaleY="3" />
  </path>
  </tam>

  <tam title="Reverse Cut the Diamond"
       from="Left-Hand Diamonds"
       formation="Diamonds LH Girl Points">
    <path>
      <move select="Hinge Left" beats="2"/>
      <move select="Lead Left" beats="2" scaleX="1.5"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleX=".5" scaleY="3"/>
    </path>
    <path>
      <move select="Hinge Left" beats="2"/>
      <move select="Lead Left" beats="2" scaleX="1.5"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleX=".5" scaleY="3"/>
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond"
       from="Facing Diamonds, Right-Hand Wave"
       formation="Diamonds Facing Girl Points">
    <path>
      <move select="Hinge Right" beats="2" />
      <move select="Lead Right" beats="2" scaleX="1.5" />
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleX=".5" scaleY="3" />
    </path>
    <path>
      <move select="Hinge Right" beats="2" />
      <move select="Lead Right" beats="2" scaleX="1.5" />
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleX=".5" scaleY="3" />
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond"
       from="Facing Diamonds, Left-Hand Wave"
       formation="Diamonds Facing LH Girl Points">
    <path>
      <move select="Hinge Left" beats="2" />
      <move select="Lead Left" beats="2" scaleX="1.5" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX=".5" scaleY="3" />
    </path>
    <path>
      <move select="Hinge Left" beats="2" />
      <move select="Lead Left" beats="2" scaleX="1.5" />
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX=".5" scaleY="3" />
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond"
       from="Right-Hand Interlocked Diamonds"
       formation="Interlocked Diamonds RH Girl Points">
    <path>
      <move select="Run Right" beats="4" scaleY="1.25"/>
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX=".5" scaleY="3" />
    </path>
    <path>
      <move select="Flip Left" beats="4" scaleY="1.25"/>
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX=".5" scaleY="3" />
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond"
       from="Left-Hand Interlocked Diamonds"
       formation="Interlocked Diamonds LH Girl Points">
    <path>
      <move select="Flip Left" beats="4" scaleY="1.25"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleX=".5" scaleY="3"/>
    </path>
    <path>
      <move select="Run Right" beats="4" scaleY="1.25"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleX=".5" scaleY="3"/>
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond"
       from="Interlocked Diamonds 3">
    <formation>
      <dancer gender="boy" x="0" y="-3" angle="180"/>
      <dancer gender="girl" x="-3" y="-2" angle="270"/>
      <dancer gender="boy" x="0" y="-1" angle="180"/>
      <dancer gender="girl" x="3" y="-2" angle="90"/>
    </formation>
    <path>
      <move select="Run Right" beats="4" scaleY="1.25"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleX=".5" scaleY="3"/>
    </path>
    <path>
      <move select="Flip Left" beats="4" scaleY="1.25"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleX=".5" scaleY="3"/>
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond"
       from="Interlocked Diamonds 4">
    <formation>
      <dancer gender="boy" x="0" y="-3" angle="0"/>
      <dancer gender="girl" x="-3" y="-2" angle="90"/>
      <dancer gender="boy" x="0" y="-1" angle="0"/>
      <dancer gender="girl" x="3" y="-2" angle="270"/>
    </formation>
    <path>
      <move select="Flip Left" beats="4" scaleY="1.25"/>
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX=".5" scaleY="3"/>
    </path>
    <path>
      <move select="Run Right" beats="4" scaleY="1.25"/>
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleX=".5" scaleY="3"/>
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond" from="Right-Hand Point-to-Point Diamonds" formation="Diamonds RH PTP Girl Points">
  <path>
    <move select="Hinge Right" beats="1"/>
    <move select="Quarter Right" beats="3" offsetX="3"/>
  </path>
  <path>
    <move select="Lead Right" beats="4" />
  </path>
  <path>
    <move select="Hinge Right" beats="1"/>
    <move select="Lead Right" beats="3" scaleX="3" scaleY="2"/>
  </path>
  <path>
    <move select="Lead Right" beats="4" scaleY="3"/>
  </path>
  </tam>

  <tam title="Reverse Cut the Diamond" from="Left-Hand Point-to-Point Diamonds" formation="Diamonds LH PTP Girl Points">
    <path>
      <move select="Hinge Left" beats="1"/>
      <move select="Lead Left" beats="3" scaleX="3" scaleY="2"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" />
    </path>
    <path>
      <move select="Hinge Left" beats="1"/>
      <move select="Quarter Left" beats="3" offsetX="3"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleY="3"/>
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond"
       from="Facing Point-to-Point Diamonds, Right-Hand Centers">
    <formation>
      <dancer gender="boy" x="-1" y="-3" angle="90"/>
      <dancer gender="girl" x="0" y="-1" angle="180"/>
      <dancer gender="boy" x="1" y="-3" angle="270"/>
      <dancer gender="girl" x="0" y="-5" angle="0"/>
    </formation>
    <path>
      <move select="Hinge Right" beats="1"/>
      <move select="Quarter Right" beats="3" offsetX="3"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" />
    </path>
    <path>
      <move select="Hinge Right" beats="1"/>
      <move select="Lead Right" beats="3" scaleX="3" scaleY="2"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleY="3"/>
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond" from="Facing Point-to-Point Diamonds, Left-Hand Centers">
    <formation>
      <dancer gender="boy" x="-1" y="-3" angle="270"/>
      <dancer gender="girl" x="0" y="-1" angle="0"/>
      <dancer gender="boy" x="1" y="-3" angle="90"/>
      <dancer gender="girl" x="0" y="-5" angle="180"/>
    </formation>
    <path>
      <move select="Hinge Left" beats="1"/>
      <move select="Lead Left" beats="3" scaleX="3" scaleY="2"/>
    </path>
    <path>
      <move select="Lead Right" beats="4" />
    </path>
    <path>
      <move select="Hinge Left" beats="1"/>
      <move select="Quarter Left" beats="3" offsetX="3"/>
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleY="3"/>
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond" from="Point-to-Point Diamonds 3">
    <formation>
      <dancer gender="boy" x="-1" y="-3" angle="90"/>
      <dancer gender="girl" x="0" y="-1" angle="0"/>
      <dancer gender="boy" x="1" y="-3" angle="90"/>
      <dancer gender="girl" x="0" y="-5" angle="180"/>
    </formation>
    <path>
      <move select="Run Right" beats="4" offsetX="1" scaleY="2"/>
    </path>
    <path>
      <move select="Stand" beats="2"/>
      <move select="Lead Right" beats="2" />
    </path>
    <path>
      <move select="Flip Left" beats="4" offsetX="1" scaleY="2"/>
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleY="3"/>
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond" from="Point-to-Point Diamonds 4">
    <formation>
      <dancer gender="boy" x="-1" y="-3" angle="270"/>
      <dancer gender="girl" x="0" y="-1" angle="0"/>
      <dancer gender="boy" x="1" y="-3" angle="270"/>
      <dancer gender="girl" x="0" y="-5" angle="180"/>
    </formation>
    <path>
      <move select="Flip Left" beats="4" offsetX="-1" scaleY="2"/>
    </path>
    <path>
      <move select="Lead Right" beats="4" />
    </path>
    <path>
      <move select="Run Right" beats="4" offsetX="-1" scaleY="2"/>
    </path>
    <path>
      <move select="Lead Right" beats="4" scaleY="3"/>
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond" from="Point-to-Point Diamonds 5">
    <formation>
      <dancer gender="boy" x="-1" y="-3" angle="270"/>
      <dancer gender="girl" x="0" y="-1" angle="180"/>
      <dancer gender="boy" x="1" y="-3" angle="270"/>
      <dancer gender="girl" x="0" y="-5" angle="0"/>
    </formation>
    <path>
      <move select="Flip Left" beats="4" offsetX="-1" scaleY="2"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" />
    </path>
    <path>
      <move select="Run Right" beats="4" offsetX="-1" scaleY="2"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleY="3"/>
    </path>
  </tam>

  <tam title="Reverse Cut the Diamond" from="Point-to-Point Diamonds 6">
    <formation>
      <dancer gender="boy" x="-1" y="-3" angle="90"/>
      <dancer gender="girl" x="0" y="-1" angle="180"/>
      <dancer gender="boy" x="1" y="-3" angle="90"/>
      <dancer gender="girl" x="0" y="-5" angle="0"/>
    </formation>
    <path>
      <move select="Run Right" beats="4" offsetX="1" scaleY="2"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" />
    </path>
    <path>
      <move select="Flip Left" beats="4" offsetX="1" scaleY="2"/>
    </path>
    <path>
      <move select="Lead Left" beats="4" scaleY="3"/>
    </path>
  </tam>

</tamination>
