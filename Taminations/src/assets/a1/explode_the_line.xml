<tamination title="Explode the Line">

  <tam title="Explode the Line"
       from="Lines Facing Out"
       formation="Lines Facing Out">
    <path>
      <move select="Quarter Left" beats="2" offsetX="-1" offsetY="1" />
      <move select="Pull Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
    <path>
      <move select="Lead Left" beats="3" />
      <move select="Forward 2" beats="3" />
    </path>
    <path>
      <move select="Lead Right" beats="3" scaleX="2" />
      <move select="Extend Right" beats="3" scaleX="2" />
    </path>
    <path>
      <move select="Quarter Right" beats="2" offsetX="-1" offsetY="-1" />
      <move select="Pull Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
    </path>
  </tam>

