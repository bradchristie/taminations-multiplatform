<tamination title="Coordinate">

  <tam title="Coordinate"
       from="Right-Hand Columns"
       formation="Column RH GBGB"
       parts="2;1;2.5" difficulty="1">
    <path>
      <move select="Run Right" beats="2" />
      <move select="Forward" />
      <move select="Swing Right" beats="2.5" scaleX=".5"/>
    </path>
    <path>
      <move select="Forward" scaleX="2" beats="2" />
      <move select="Lead Right" />
      <move select="Stand" beats="2" hands="none"/>
      <move select="Lead Right" scaleX="3" scaleY="2" beats="2.5" />
    </path>
    <path>
      <move select="Forward 2" />
      <move select="Forward"/>
      <move select="Swing Right" beats="2.5" scaleX=".5"/>
    </path>
    <path>
      <move select="Forward 2" />
      <move select="Forward"/>
      <move select="Swing Right" beats="2.5" scaleX=".5"/>
      <move select="Extend Left" beats="2.5" scaleX="2" scaleY="2"/>
    </path>
  </tam>

  <tam title="Coordinate"
       from="Left-Hand Columns"
       formation="Column LH GBGB"
       parts="2;1;2.5" difficulty="2">
    <path>
      <move select="Forward 2" />
      <move select="Forward"/>
      <move select="Swing Left" beats="2.5" scaleX=".5"/>
      <move select="Extend Right" beats="2.5" scaleX="2" scaleY="2"/>
    </path>
    <path>
      <move select="Forward 2" />
      <move select="Forward"/>
      <move select="Swing Left" beats="2.5" scaleX=".5"/>
    </path>
    <path>
      <move select="Forward" scaleX="2" beats="2" />
      <move select="Lead Left" />
      <move select="Stand" beats="2" hands="none"/>
      <move select="Lead Left" scaleX="3" scaleY="2" beats="2.5" />
    </path>
    <path>
      <move select="Run Left" beats="2" />
      <move select="Forward" />
      <move select="Swing Left" beats="2.5" scaleX=".5"/>
    </path>
  </tam>

  <tam title="Coordinate"
       from="3 and 1 Lines, Right-Hand Centers"
       parts="4;2;3" difficulty="3">
    <formation>
      <dancer gender="boy" x="-2" y="3" angle="0"/>
      <dancer gender="girl" x="-2" y="1" angle="0"/>
      <dancer gender="boy" x="-2" y="-1" angle="180"/>
      <dancer gender="girl" x="-2" y="-3" angle="0"/>
    </formation>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Lead Right" beats="2" scaleX="3" scaleY="3" />
      <move select="Stand" beats="3" hands="none"/>
      <move select="Lead Right" beats="3" scaleX="3" scaleY="3" />
    </path>
    <path>
      <move select="Forward 4" />
      <move select="Lead Right" beats="2" scaleX=".5" />
      <move select="Swing Left" scaleX=".75" scaleY=".5" />
      <move select="Dodge Left" scaleY=".25" hands="left"/>
    </path>
    <path>
      <move select="Run Right" beats="4" />
      <move select="Forward 2" />
      <move select="Swing Right" />
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2" />
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5" />
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5" />
      <move select="Lead Left" beats="2" scaleX="2" scaleY="3" />
      <move select="Swing Left" scaleX=".75" scaleY="1.25" />
      <move select="Dodge Right" scaleY=".25" hands="left"/>
    </path>
    <taminator>
      Not officially in the Plus program, but can be workshopped,
      or called at higher levels.
    </taminator>
  </tam>

  <tam title="Coordinate" from="3 and 1 Lines, Left-Hand Centers"
       formation="3 and 1 lines #2" difficulty="3" parts="4;2;3">
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Right" beats="2" scaleX="3" scaleY="3" />
      <move select="Stand" beats="3" hands="none"/>
      <move select="Lead Right" beats="3" scaleX="4" scaleY="3"/>
    </path>
    <path>
      <move select="Run Left" beats="4"/>
      <move select="Forward 2"/>
      <move select="Swing Left"/>
      <move select="Extend Right" beats="3" scaleX="2"/>
    </path>
    <path>
      <move select="Forward 4"/>
      <move select="Lead Left" beats="2" scaleX=".5"/>
      <move select="Run Right" scaleX=".75" scaleY=".5"/>
      <move select="Dodge Right" scaleY=".25" hands="right"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Left" beats="2" scaleX="2" scaleY="3" />
      <move select="Flip Left" scaleY="1.25"/>
      <move select="Dodge Right" scaleY=".25" hands="left"/>
    </path>
    <taminator>
      Not officially in the Plus program, but can be workshopped,
      or called at higher levels.
    </taminator>
  </tam>

  <tam title="Coordinate" from="Normal Lines"
       formation="Normal Lines"
       parts="4;2;3" difficulty="3">
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Right" beats="2" scaleX="2.5" scaleY="3"/>
      <move select="Stand" hands="none" beats="3"/>
      <move select="Lead Right" beats="3" scaleX="3" scaleY="2.5"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Right" beats="2" scaleX=".5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".25"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Quarter Left" beats="2" offsetX="-1" offsetY="1"/>
      <move select="Swing Left" scaleY=".75"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY="1.5"/>
    </path>
    <path>
      <move select="Extend Left" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Extend Right" beats="2" scaleX="2" scaleY=".5"/>
      <move select="Lead Left" beats="2" scaleX="1.5" scaleY="3"/>
      <move select="Swing Left" scaleX=".5" scaleY="1"/>
      <move select="Dodge Right" beats="3" hands="left" scaleY=".25"/>
    </path>
    <taminator>
      Not officially in the Plus program, but can be workshopped,
      or called at higher levels.
    </taminator>
  </tam>

  <tam title="Coordinate" from="Lines Facing Out"
       formation="Lines Facing Out"
       parts="4;1;3" difficulty="3">
    <path>
      <move select="Lead Left" beats="2" scaleX="1.5" scaleY="3"/>
      <move select="Lead Left" beats="2" scaleX="3" scaleY="2.5"/>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Dodge Right" beats="3" hands="left" scaleY=".25"/>
    </path>
    <path>
      <move select="Quarter Left" beats="2" offsetX="-.5" offsetY="1"/>
      <move select="Lead Left" beats="2" scaleY=".5"/>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Extend Right" beats="3" scaleX="3" scaleY="1.5"/>
    </path>
    <path>
      <move select="Lead Right" beats="2" scaleX=".5"/>
      <move select="Lead Right" beats="2" scaleY="1.5"/>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Swing Left" scaleX=".5" scaleY=".5"/>
      <move select="Dodge Right" beats="3" hands="left" scaleY=".25"/>
    </path>
    <path>
      <move select="Lead Right" beats="2" scaleX="2.5" scaleY="3"/>
      <move select="Lead Right" beats="2" scaleX="3" scaleY="3.5"/>
      <move select="Extend Left" scaleY=".5"/>
      <move select="Stand" beats="3" hands="none"/>
      <move select="Lead Right" beats="3" scaleX="3" scaleY="1.5"/>
    </path>
    <taminator>
      Not officially in the Plus program, but can be workshopped,
      or called at higher levels.
    </taminator>
  </tam>

  <tam title="Coordinate" from="T-Bones 1" formation="T-Bone DLDL"
       parts="4;1;3" difficulty="3">
    <path>
      <move select="Run Left" beats="4" scaleX="2" scaleY="3"/>
      <move select="Forward"/>
      <move select="Swing Left"/>
    </path>
    <path>
      <move select="Forward 2" beats="4"/>
      <move select="Lead Right" scaleX="1.5" beats="1"/>
      <move select="Stand" beats="3" hands="none"/>
      <move select="Lead Right" beats="3" scaleX="3" scaleY="2.5"/>
    </path>
    <path>
      <move select="Run Right" beats="4"/>
      <move select="Forward"/>
      <move select="Swing Left"/>
    </path>
    <path>
      <move select="Forward 2" beats="4"/>
      <move select="Forward"/>
      <move select="Swing Right"/>
      <move select="Extend Left" beats="3" scaleX="2" scaleY="2"/>
    </path>
    <taminator>
      Not officially in the Plus program, but can be workshopped,
      or called at higher levels.
    </taminator>
  </tam>

  <tam title="Coordinate" from="T-Bones 2" formation="T-Bone RDRD"
       parts="4;1;3" difficulty="3">
    <path>
      <move select="Run Right" beats="4" scaleX="2" scaleY="3"/>
      <move select="Forward"/>
      <move select="Swing Right"/>
    </path>
    <path>
      <move select="Forward 2" beats="4"/>
      <move select="Lead Left" scaleX="1.5" beats="1"/>
      <move select="Stand" beats="3" hands="none"/>
      <move select="Lead Left" beats="3" scaleX="3" scaleY="2.5"/>
    </path>
    <path>
      <move select="Run Left" beats="4"/>
      <move select="Forward"/>
      <move select="Swing Right"/>
    </path>
    <path>
      <move select="Forward 2" beats="4"/>
      <move select="Forward"/>
      <move select="Swing Left"/>
      <move select="Extend Right" beats="3" scaleX="2" scaleY="2"/>
    </path>
    <taminator>
      Not officially in the Plus program, but can be workshopped,
      or called at higher levels.
    </taminator>
  </tam>

  <tam title="Coordinate" from="T-Bones 3" formation="T-Bone LDLD"
       parts="4;1;3" difficulty="3">
    <path>
      <move select="Run Right" beats="4"/>
      <move select="Forward"/>
      <move select="Swing Right" offsetX=".5"/>
    </path>
    <path>
      <move select="Run Left" beats="4"/>
      <move select="Forward"/>
      <move select="Swing Left" scaleY=".75"/>
      <move select="Extend Right" beats="3" scaleX="3"/>
    </path>
    <path>
      <move select="Forward 2" beats="4"/>
      <move select="Forward"/>
      <move select="Swing Right" offsetX="-.5" scaleX=".5"/>
    </path>
    <path>
      <move select="Run Right" beats="4" scaleX="2" scaleY="3"/>
      <move select="Forward"/>
      <move select="Dodge Left" beats="1" scaleY=".25"/>
      <move select="Stand" beats="2"/>
      <move select="Lead Right" beats="3" scaleX="3" scaleY="2"/>
    </path>
    <taminator>
      Not officially in the Plus program, but can be workshopped,
      or called at higher levels.
    </taminator>
  </tam>

  <tam title="Coordinate" from="T-Bones 4" formation="T-Bone DRDR"
       parts="4;1;3" difficulty="3">
    <path>
      <move select="Run Left" beats="4" scaleX="2" scaleY="3"/>
      <move select="Forward"/>
      <move select="Dodge Right" beats="1" scaleY=".25"/>
      <move select="Stand" beats="2"/>
      <move select="Lead Left" beats="3" scaleX="3" scaleY="2"/>
    </path>
    <path>
      <move select="Forward 2" beats="4"/>
      <move select="Forward"/>
      <move select="Swing Left" offsetX="-.5" scaleX=".5"/>
    </path>
    <path>
      <move select="Run Right" beats="4"/>
      <move select="Forward"/>
      <move select="Swing Right" scaleY=".75"/>
      <move select="Extend Left" beats="3" scaleX="3"/>
    </path>
    <path>
      <move select="Run Left" beats="4"/>
      <move select="Forward"/>
      <move select="Swing Left" offsetX=".5"/>
    </path>
    <taminator>
      Not officially in the Plus program, but can be workshopped,
      or called at higher levels.
    </taminator>
  </tam>

</tamination>
